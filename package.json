{
  "name": "keybind-modes",
  "displayName": "Keybind Modes",
  "description": "Create keybinds leveraging tmux/zellij-modes. Stop chord-induced RSI today",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other",
    "Keymaps"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "keybind-modes.toggle",
        "title": "Toggle Keybind Mode"
      },
      {
        "command": "keybind-modes.clear",
        "title": "Clear Keybind Mode"
      }
    ],
    "keybindings": [
      {
        "command": "keybind-modes.clear",
        "key": "escape",
        "when": "mode != default"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.96.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.25.0",
    "@typescript-eslint/parser": "^8.25.0",
    "eslint": "^9.21.0",
    "typescript": "^5.7.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}